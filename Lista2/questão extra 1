using System;

class Program
{
    public static void Main(string[] args)
    {
        Console.WriteLine("Informe um número: ");
        double num = double.Parse(Console.ReadLine());
    
        if (num % 3 == 0 && num % 6 == 0)
        {
          Console.WriteLine($"{num} é divisível por 3 e por 6");
        }
      else if (num % 3 == 0)
      {
         Console.WriteLine($"{num} é divisível apenas por 3");
      }
      else if (num % 6 == 0)
      {
         Console.WriteLine($"{num} é divisível apenas por 6");
      }
      else 
      {
         Console.WriteLine($"{num} não é divisível por 3 e nem por 6");
      }
      Console.ReadLine();
    }
}

