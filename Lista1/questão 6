using System;

class Program
{

    public static void TrocarValores(double varA, double varB)
    {
        double varC = varA;
        varA = varB;
        varB = varC;
    }

    public static void Main(string[] args)
    {
        Console.WriteLine("Informe um valor para a vari치vel A: ");
        double varA = double.Parse(Console.ReadLine());
        Console.WriteLine("Informe um valor para a vari치vel B: ");
        double varB = double.Parse(Console.ReadLine());
        TrocarValores(ref varA, ref varB);
        Console.WriteLine($"Var치vel A: {varA}");
        Console.WriteLine($"Vari치vel B: {varB}");

        Console.ReadLine();
    }
}
